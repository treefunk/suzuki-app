<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/img_main_bg"
    >


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_extract_data"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0"

        android:layout_marginTop="24dp"
        tools:listitem="@layout/item_extract_data"
        tools:itemCount="2"
        />

    <Button
        android:id="@+id/btn_extract_all_data"
        android:layout_width="wrap_content"
        android:layout_height="32.5dp"

        app:layout_constraintTop_toBottomOf="@id/rv_extract_data"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0"

        style="@style/suzukiButton3"
        android:text="Extract all data"
        app:backgroundTint="@color/color_blue_300"
        />








</androidx.constraintlayout.widget.ConstraintLayout>